<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/f4main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    >

    <RelativeLayout
        android:id="@+id/fotter"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:layout_alignParentBottom="true"
        android:background="#666666"
        android:gravity="center">

        <LinearLayout
            android:id="@+id/homesetFotterD1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:clickable="true"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="#666666"
                app:srcCompat="@drawable/icon_tab_bar_home" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Home"
                android:textColor="#FFFFFF" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/inputsetFotterD1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/yoyakusetFotterD1"
            android:layout_toRightOf="@+id/homesetFotterD1"
            android:clickable="true"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="#666666"
                app:srcCompat="@drawable/icon_tab_bar_input" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center_horizontal"
                android:text="入力"
                android:textColor="#FFFFFF" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/yoyakusetFotterD1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:clickable="true"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="#666666"
                app:srcCompat="@drawable/icon_tab_bar_view" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="予定／履歴"
                android:textColor="#FFFFFF" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/hospitalSetFotterD1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/mypagesetFotterD1"
            android:layout_toRightOf="@+id/yoyakusetFotterD1"
            android:clickable="true"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="87dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="#666666"
                app:srcCompat="@drawable/icon_tab_bar_search" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:autoText="false"
                android:gravity="center_horizontal"
                android:text="病院検索"
                android:textColor="#FFFFFF" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mypagesetFotterD1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:clickable="true"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="#666666"
                app:srcCompat="@drawable/icon_tab_bar_mypage" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="マイページ"
                android:textColor="#FFFFFF" />
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/header_home"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/shape_ivD1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20sp"
            android:clickable="true"
            android:scaleType="fitXY"
            android:src="@drawable/shape"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="16dp"
            tools:name="Shape" />

        <TextView
            android:id="@+id/通院予定label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:text="ヘルプ"
            android:textColor="#666666"
            android:textSize="16dp"
            tools:name="各種設定" />

        <ImageView
            android:id="@+id/button_bell_ivD1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15sp"
            android:clickable="true"
            android:scaleType="fitXY"
            android:src="@drawable/button_bell"
            tools:layout_editor_absoluteX="280dp"
            tools:layout_editor_absoluteY="10dp"
            tools:name="Button/ButtonHelp" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/rectangle4G8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/header"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="15dp"
        android:orientation="horizontal"
        tools:name="Rectangle">

        <ImageView
            android:id="@+id/imageButton5G8"
            android:layout_width="5dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:background="#0CBBB6"
            app:srcCompat="@drawable/rectangle" />

        <TextView
            android:id="@+id/昨年1月12月の間に1つの会社のみlabelG8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="0dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="3dp"
            android:gravity="left"
            android:text="Q. 画像の正しい取り方は？"
            android:textColor="#333333"
            android:textSize="17sp"
            tools:name="・昨年1月〜12月の間に、1つの会社のみ" />

    </LinearLayout>

    <TextView
        android:id="@+id/本アプリでは画像解析等に時間が掛かる場合label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rectangle4G8"
        android:layout_marginLeft="10sp"
        android:layout_marginRight="10sp"
        android:layout_marginTop="10sp"
        android:gravity="left"
        android:text="A.書類を撮影するときに、傾いたり文字がボケたりしてないかご確認ください。"
        android:textColor="#666666"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="63dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/本アプリでは画像解析等に時間が掛かる場合label"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="15dp"
        android:background="#F4F4F4"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:text="それでも解決しない場合は、以下のお問い合わせボタンよりメールでお問い合わせください。" />

        <TextView
            android:id="@+id/toiawaseD1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="10sp"
            android:layout_marginRight="10sp"
            android:layout_marginTop="10sp"
            android:layout_weight="1"
            android:background="@drawable/button_round"
            android:clickable="true"
            android:gravity="center"
            android:text="お問い合わせ"
            android:textColor="#FFFFFF" />

    </LinearLayout>
</RelativeLayout>

